{
  "name": "prelevements-deau-front",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "update-icons": "rm -rf .next && yarn react-dsfr update-icons",
    "dev": "yarn react-dsfr update-icons && next dev",
    "build": "yarn react-dsfr update-icons && next build",
    "start": "next start",
    "lint": "xo",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.0",
    "@fabnum/prelevements-deau-timeseries-parsers": "0.0.7",
    "date-fns": "^4.1.0",
    "flexsearch": "^0.7.43",
    "lodash-es": "^4.17.21",
    "maplibre-gl": "^5.3.0",
    "next": "^15.4.7",
    "next-auth": "^4.24.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@codegouvfr/react-dsfr": "^1.25.3",
    "@mui/icons-material": "^6.4.10",
    "@mui/material": "^6.4.10",
    "@mui/material-nextjs": "^6.4.3",
    "@mui/system": "^6.4.10",
    "@mui/x-charts": "^7.28.0",
    "@mui/x-data-grid": "^7.28.2",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/nextjs": "^9.1.3",
    "eslint-config-xo-nextjs": "^6.3.0",
    "postcss": "^8.5.3",
    "sass": "^1.86.1",
    "sass-loader": "^16.0.5",
    "storybook": "^9.1.3",
    "tailwindcss": "^3.4.17",
    "xo": "^0.59.3"
  },
  "xo": {
    "extends": "xo-nextjs",
    "envs": [
      "browser",
      "node",
      "es2021"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "globals": [
      "React"
    ],
    "semicolon": false,
    "space": 2,
    "rules": {
      "react/prop-types": "off",
      "unicorn/prevent-abbreviations": "off",
      "unicorn/prefer-export-from": "off",
      "unicorn/import-style": "off",
      "unicorn/prefer-ternary": "off",
      "unicorn/prefer-module": "off",
      "unicorn/prefer-node-protocol": "off",
      "n/prefer-global/process": "off",
      "n/prefer-global/buffer": "off",
      "import/no-unassigned-import": "off",
      "comma-dangle": [
        "error",
        "never"
      ],
      "import/order": [
        "error",
        {
          "groups": [
            "builtin",
            "external",
            "internal",
            "parent",
            "sibling",
            "index"
          ],
          "pathGroups": [
            {
              "pattern": "react",
              "group": "builtin",
              "position": "before"
            },
            {
              "pattern": "routes/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "services/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "helpers/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "util/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "contexts/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "hoc/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "hooks/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "components/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "containers/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "assets/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "styles/**",
              "group": "internal",
              "position": "before"
            }
          ],
          "pathGroupsExcludedImportTypes": [
            "react"
          ],
          "newlines-between": "always",
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": true
          }
        }
      ]
    }
  },
  "engines": {
    "node": ">= 22.11 < 23"
  },
  "packageManager": "yarn@4.6.0",
  "author": "Théophile Merlière <theophile.merliere@gmail.com>",
  "license": "MIT"
}
